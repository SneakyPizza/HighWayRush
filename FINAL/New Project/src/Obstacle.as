package src{	import flash.events.Event;	import flash.display.Sprite;	import flash.display.MovieClip;	/**	 * ...	 * @author Coen Meijer	 */	public class Obstacle extends MovieClip	{				public var art:MovieClip;		public var life:int		public var speed:int;				public function Obstacle() {			addEventListener(Event.ADDED_TO_STAGE,init);		}				public function init(e:Event):void		{			art = new CarArt();			addChild(art);						life = 1;			speed = 12;					}		public function obstacleMovement():void{			this.x -= speed;			}		public function destroy() {			var explo:Explosion = new Explosion();						if (this.x >= 0) {				Main._main.explosions.push(explo)				Main._main.addChild(explo);				explo.x = this.x;				explo.y = this.y;			}		}	}}